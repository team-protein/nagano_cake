<script>
  var products = <%== JSON.dump(@product_names_all) %>;
  var products = products.split(",").map(product => product = {value: product})
  $('.suggest').autocomplete({
    source: products,
    delay: 100, // 入力してからサジェストが動くまでの時間(ms)
    minLength: 2, // 2文字入力しないとサジェストが動かない
    // サジェストが選択された際、そのワードで勝手に検索するようにする
    select : function(event, ui) {
      $("#words").val(ui.item.value);
      document.words.submit();
    }
  });
</script>